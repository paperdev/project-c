{
  "name": "nesxt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "__NOTE__": "rimraf for Windows OS",
    "clean": "rm -rf server/dist client/.next",
    "server:dev": "cd server && pnpm run start:dev",
    "server": "cd server && pnpm run start",
    "client:dev": "cd client && pnpm run dev",
    "client": "cd client && pnpm run start",
    "format": "prettier --write \"server/**/*.ts\" \"client/**/*.ts\" \"client/**/*.tsx\"",
    "lint": "eslint \"**/*.ts\" \"**/*.tsx\" --fix"
  },
  "dependencies": {
    "@nestjs/cli": "^10.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3"
  }
}
